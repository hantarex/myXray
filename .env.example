# 3X-UI Panel Configuration
PANEL_PORT=2053
PANEL_USERNAME=admin
PANEL_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD

# SSL Configuration (для автоматической установки через acme.sh)
# Заполните DOMAIN и EMAIL перед запуском scripts/setup-ssl.sh
DOMAIN=your-domain.com
EMAIL=your-email@example.com

# SSL Settings (опционально, настраивается через панель после получения сертификата)
SSL_ENABLED=false                        # Включить SSL для панели
SSL_CERT_PATH=/root/cert/fullchain.pem  # Путь к сертификату внутри контейнера
SSL_KEY_PATH=/root/cert/privkey.pem     # Путь к приватному ключу внутри контейнера

# Xray Configuration
XRAY_LOG_LEVEL=warning

# Port Configuration
VLESS_PORT=443
VMESS_PORT=8443
TROJAN_PORT=8080
SHADOWSOCKS_PORT=9443

# Backup Configuration
BACKUP_DIR=./backups
BACKUP_RETENTION_DAYS=7

# Timezone
TZ=Europe/Moscow
